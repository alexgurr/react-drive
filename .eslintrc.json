{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/require-default-props": "off",
    "react/jsx-filename-extension": "off",
    "comma-dangle": "off",
    "no-void": "off",
    "consistent-return": ["error", { "treatUndefinedAsUnspecified": true }]
  }
}
